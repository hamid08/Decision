
@model IList<Decision.ViewModel.Home.NewAddedApplicantViewModel>
    

    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title">
                <span class="fa fa-list-alt"></span>
                آخرین اساتید ثبت شده
            </h3>
        </div>
        <div class="panel-body">
            <ul class="list-group">
                @foreach (var newAddedApplicantViewModel in Model)
                {
                    <li class="list-group-item log-list">
                        <span>@newAddedApplicantViewModel.FullName</span>
                        
                        <span class="badge">@newAddedApplicantViewModel.CreateDate.ToRemainingDateTime()</span>
                    </li>
                }
               

            </ul>
        </div>
        <div class="panel-footer">
            <a href="@Url.Action(MVC.Applicant.List())">
                موارد بیشتر
                <span class="fa fa-arrow-left"></span>
            </a>
        </div>
    </div>
